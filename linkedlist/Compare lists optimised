struct node* comapreLists(struct node *headA,struct node *headB)
{
    while(headA!=NULL && headB!=NULL)
    {
        if(headA->data != headB->data)
            return 0;
        else
        {
            headA = headA->next;
            headB = headB->next;
        }
    }
    return headA==NULL && headB==NULL ? 1 : 0;
}

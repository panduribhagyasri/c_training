struct CLLNode* deleteAtBeginning(struct CLLNode *tail)
{
    if(tail == NULL)    return tail; //return NULL
    
    if(tail->next == tail)
    {
        free(tail);
        return NULL;
    }
    
    struct CLLNode *temp = tail->next;
    tail->next = tail->next->next; //temp->next;
    temp->next = NULL;
    free(temp);
    return tail;
    
    /*if(tail==NULL || tail->next==tail)    return NULL;
    else
    {
        tail->next = tail->next->next;
        return tail;
    }*/
}

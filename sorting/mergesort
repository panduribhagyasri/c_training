#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void merge(int arr[],int l,int m,int r)
{
    int n1 = m-l+1;
    int la[n1],i;
    for(i=0;i<n1;i++)
        la[i]= arr[l+i];
    int n2 = r-m;
    int ra[n2],j;
    for(j=0;j<n2;j++)
        ra[j]=arr[m+j+1];
    i=0;j=0;
    int k=l;
    while(i<n1 && j<n2)
        arr[k++]=la[i],ra[j] ? la[i++];ra[j++];
    while(i<n1)
        arr
        //need to complete
}
void mergeSort(int arr[],int l,int r)
{
    if(l < r)
    {
        int m = (l+r)/2;
        mergeSort(arr,l,m);
        mergeSort(arr,m+1,r);
        merge(arr,l,m,r);
    }
}


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<stdbool.h>
void add1(void);
int main()
{
    add1();
    return 0;
}
void add1(void)
{
    int x,y;
    scanf("%d %d",&x,&y);
    printf("sum from I:%d",x+y);
    
}

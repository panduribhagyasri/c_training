#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdbool.h>

struct stackNode
{
    int data;
    struct stackNode *next;
}*top = NULL;

struct stackNode* push(struct stackNode *top,int newValue)
{
    struct stackNode *newNode = (struct stackNode*) malloc(sizeof(struct stackNode));
    newNode->data = newValue;
    newNode->next = top;
    return newNode;
}


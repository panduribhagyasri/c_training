int main() 
{
    top ? printf("\nTop element: %d\n",top->data) : printf("\nStack is empty\n");
    for(int i=1;i<=6;i++)
        top = push(top,i*2);
    
    top = push(top,15);
    
    display(top);
    
    top ? printf("\nTop element: %d",top->data) : printf("\nStack is empty");
    
    top = pop(top);
    
    display(top);
    
    isEmpty(top) ? printf("\nList is empty") : printf("\nStack is not Empty");
    
    top ? printf("\nTop element: %d\n",top->data) : printf("\nStack is empty\n");
    
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
bool isEmpty()
{
    return top==-1 ? true : false;
    
}
bool isFull()
{
    return top>=MAX-1 ? true : false;
    
}
void push(int n)
{
    if(isFull())
    {
        printf("\nOverflow... There is no space to place the data");
        return;
    }
    stack[++top] = n;
}
void display()
{
    if(isEmpty())
    {
        printf("\nElements are not existed.i.e, underflow");
        return;
    }
    for(int i=top;i>=0;i--)
        printf("\nPosition :%d => value:%d",i,stack[i]);
}
void pop()
{
    if(isEmpty())
    {
        printf("\n Elements are not exixted to delete.");
        return;
    }
    top--;
}
int size()
{
    
}
void peek()
{
    //code incomplete
}
int main()
{
    printf("total no of elements in the stack is:%d",size());
    push(10);
    push(20);
    push(30);
    push(40);
    push(50);
    push(60);//overflow
    display();
    printf("total no of elements in the stack is:%d",size());
    pop();
    pop();
    display();
    printf("total no of elements in the stack is:%d",size());
    peek();
    return 0;
}

